package OOP;

public class Student {
	
		String name, gender, dept;
		int id;
		
		
		void setInfo(int id, String n, String g, String d) {
			
			this.id = id;
			this.name = n;
			this.gender= g;
			this.dept = d;
		}
		
		
		void displayInfo(){
			
			System.out.println("Id: "+id+"\n"+"Name: "+name+"\n"+"Gender: "+gender+"\n"+ "Dept: "+ dept);
		}

}
