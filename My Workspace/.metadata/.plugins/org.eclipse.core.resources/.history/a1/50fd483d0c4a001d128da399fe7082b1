package OOP;

public class TestStudent {
	public static void main(String[] args) {
		
		Student st = new Student();
		
		st.name = "Sourav Paul";
		st.gender = "Male";
		st.id= 29;
		st.dept = "CSE";
		
		
		st.displayInfo();
		
		
		//System.out.println("Name: "+st.name+"\n"+ "Gender: "+st.gender+"\n"+ "ID: "+st.id + "\n"+ "Dept: "+st.dept);
		
		Student st1 = new Student();
		
		st1.name = "Sourav";
		st1.gender = "Male";
		st1.id= 30;
		st1.dept = "CSE";
		System.out.println("Name: "+st1.name+"\n"+ "Gender: "+st1.gender+"\n"+ "ID: "+st1.id + "\n"+ "Dept: "+st1.dept);
		
		
	}

}
