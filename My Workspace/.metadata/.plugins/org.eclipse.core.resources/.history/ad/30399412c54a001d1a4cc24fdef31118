package This;

public class Student {

	int id;
	String name;
	String address;
	int age;
	
	Student(int id, String name, String address, int age) {
		
		this.id = id;
		this.name = name;
		this.address = address;
		this.age = age;
	}
	
	void display() {
		System.out.println(id + " "+name + " "+address+" "+age);
	}
	
	
}
