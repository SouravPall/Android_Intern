
public class DataTypes {

	public static void main(String args[]) {
		
		int a;//32 bits memory store 
		
		boolean b; //1 bits memory store
		
		byte b1;
		
		char c; // 16 bits memory store 
		
		short s; //16 bits memory store
		
		long l; //64 bits memory store
		
		float  f; //64 bits memory store
		
		double d;//64 bits memory store
		
		
		int x = 10, y = 5, z = 8;
		float avg;
		
		avg = (x+y+z)/3;
		
		System.out.println(avg);
		
		
		
		a = 10;
		b1 =126;
		b = true;
		f= 1.5f;
		l=10000;
		s= 100;
		c = 'A';
		d= 12.5;
		
		System.out.println(b+" "+ a + " "+b1+" "+ f+ " "+l+" "+c);
		
	}
	
}
